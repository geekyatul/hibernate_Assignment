/**
 * Hello world!
 *
 */
public class App 
{
    public static void main( String[] args )
    {
        System.out.println( "Hello World!" );
        
        EmployeeDao employeeDao=new EmployeeDao(); 
        
        Employee emp1=new Employee("Atul patel","B1");
        
        //save the object 
        employeeDao.saveEmployee(emp1);
        
    }
}
